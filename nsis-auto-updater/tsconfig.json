    "target": "es6",
    "noImplicitAny": true,
    "outDir": "out",
    "newLine": "LF",
    "noResolve": true,
    "noEmitOnError": true,
    "inlineSources": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
